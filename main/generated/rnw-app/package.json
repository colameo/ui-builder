{
  "name": "rnw-app",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/expo/AppEntry.js",
  "scripts": {
    "start:web": "react-app-rewired start",
    "build:web": "react-app-rewired build",
    "test:web": "react-app-rewired test --env=jsdom",
    "eject:web": "react-scripts eject",
    "start:native": "expo start",
    "eject:native": "expo eject",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test:native": "node node_modules/jest/bin/jest.js --watch",
    "test": "npm run test:web && npm run test:native"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "customize-cra": "^0.2.12",
    "expo": "^32.0.6",
    "react": "^16.8.4",
    "react-app-rewired": "^2.1.1",
    "react-art": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-native": "0.57",
    "react-native-elements": "^1.1.0",
    "react-native-vector-icons": "^6.4.2",
    "react-native-web": "^0.11.1",
    "react-scripts": "2.1.8",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "babel-preset-expo": "^5.0.0",
    "flow-bin": "^0.94.0",
    "jest-expo": "^32.0.0",
    "react-test-renderer": "^16.8.4"
  }
}
